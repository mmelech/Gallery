{% extends 'base.html.twig' %}

{% block title %}
    {{ 'edit'|trans({'%id%': photo.id|default('')}) }}
{% endblock %}
{% block body %}

    <h1>{{ 'edit'|trans({'%id%': photo.id|default('')}) }}</h1>
    {{ form_start(form, { method: 'PUT', action: url('photo_edit', {id: photo.id}) }) }}
    {{ form_widget(form) }}
    <div class="form-group row float-sm-right">
        <input type="submit" value="{{ 'action.save'|trans }}" class="btn btn-outline-success" />
    </div>
    <div>
    </div>
    {{ form_end(form) }}
{% endblock %}